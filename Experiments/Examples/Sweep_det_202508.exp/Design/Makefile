# Makefile to encode usage information
#


default: scripts

scripts:
	explode.py

doc:
	cp README.md ../

finalize: doc
	cp Staging/*.json ../

# Note: removes the Scripts - think before running

# remove all generated files while iterating on
# file naming and design
#   run/ -> the Dakota working directories
#   ../*.json -> Scripts generated by Dakota templating
clean:
	rm -fr Staging
	rm -f ../README.md
	rm -f ../*.json
	@ echo "Note: May also need rm -r sims/..."

